KERNEL_NAME=
KERNEL_VERSION=2.6.39-dreamplug

post_install() {
  # ARM doesn't use mkinitcpio or anything like that
  # so we skip it here
  echo -e "\033[1mNotice\033[0m"
  echo "This package is built specifically for DreamPlugs."
  echo "It may or may not work on other ARMv5te plug computers."
  /sbin/depmod ${KERNEL_VERSION}
}

post_upgrade() {
  post_install
}

op=$1
shift
$op $*
